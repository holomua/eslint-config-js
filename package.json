{
  "name": "eslint-config-holomua",
  "version": "0.1.1",
  "description": "JavaScript ESLint sharable config",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js ./",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier -c \"**/*.{js,md,ts,json}\"",
    "prettier:write": "npm run prettier -- --write",
    "test": "npm run lint && npm run prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holomua/eslint-config-js.git"
  },
  "keywords": [
    "eslint",
    "config",
    "holomua"
  ],
  "author": "William West <wwest@holomua.dev>",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/holomua/eslint-config-js/issues"
  },
  "homepage": "https://github.com/holomua/eslint-config-js#readme",
  "dependencies": {
    "eslint": "^7.6.0"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  }
}
